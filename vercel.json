{
  "name": "sigle",
  "version": 2,
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ],
  "builds": [{ "src": "sigle/package.json", "use": "@now/next" }],
  "build": {
    "env": {
      "APP_URL": "https://app.sigle.io",
      "FATHOM_SITE_ID": "DLJCUFGQ"
    }
  }
}
