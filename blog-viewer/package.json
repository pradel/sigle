{
  "name": "blog-viewer",
  "version": "0.0.1",
  "scripts": {
    "dev": "next --port 3001",
    "build": "prisma generate && next build",
    "start": "yarn db:migrate && next start",
    "db:migrate": "prisma migrate dev --preview-feature",
    "test": "yarn test:lint && yarn test:only",
    "test:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "test:only": "jest --passWithNoTests"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@prisma/client": "2.18.0",
    "@sentry/browser": "6.2.1",
    "@sentry/integrations": "6.2.1",
    "@sentry/node": "6.2.1",
    "@stacks/auth": "1.2.3",
    "date-fns": "2.16.1",
    "fathom-client": "3.0.0",
    "feed": "4.2.1",
    "immutable": "4.0.0-rc.12",
    "next": "10.0.8",
    "next-seo": "4.20.0",
    "nprogress": "0.2.0",
    "prisma": "2.18.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reading-time": "1.3.0",
    "slate": "0.47.9",
    "slate-html-serializer": "0.8.11",
    "slate-plain-serializer": "0.7.11",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@fontsource/roboto": "4.2.1",
    "@sentry/webpack-plugin": "1.14.1",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/typography": "0.3.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.28",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/slate": "0.47.4",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.7.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "babel-plugin-macros": "3.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "jest": "26.6.3",
    "sass": "1.32.8",
    "tailwindcss": "2.0.2",
    "twin.macro": "2.2.3",
    "typescript": "4.1.5"
  },
  "license": "MIT"
}
