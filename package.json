{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "test": "yarn test:lint && yarn test:only",
    "analyze": "ANALYZE=true next build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,css}'",
    "graphql:generate": "DOTENV_CONFIG_PATH=.env.local graphql-codegen --config codegen.yml -r dotenv/config",
    "test:only": "jest",
    "test:lint": "eslint 'src/**/*.{js,ts,tsx}'"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@types/slate": "0.47.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ]
  },
  "dependencies": {
    "@blockstack/connect": "2.12.7",
    "@blockstack/stacks-transactions": "0.7.0",
    "@reach/dialog": "0.12.1",
    "@reach/menu-button": "0.12.1",
    "@reach/tooltip": "0.12.1",
    "@sentry/browser": "5.29.2",
    "@sentry/integrations": "5.29.2",
    "@sentry/node": "5.29.2",
    "@tippy.js/react": "3.1.1",
    "blockstack": "21.1.1",
    "date-fns": "2.16.1",
    "fathom-client": "3.0.0",
    "feed": "4.2.1",
    "formik": "2.2.6",
    "graphql": "15.4.0",
    "graphql-request": "3.4.0",
    "immutable": "4.0.0-rc.12",
    "nanoid": "3.1.20",
    "next": "10.0.3",
    "next-seo": "4.17.0",
    "node-fetch": "2.6.1",
    "nprogress": "0.2.0",
    "react": "17.0.1",
    "react-color": "2.19.3",
    "react-div-100vh": "0.5.6",
    "react-dom": "17.0.1",
    "react-dropzone": "11.2.4",
    "react-icons": "4.1.0",
    "react-spring": "8.0.27",
    "react-toastify": "6.2.0",
    "reading-time": "1.2.1",
    "slate": "0.47.9",
    "slate-html-serializer": "0.8.11",
    "slate-plain-serializer": "0.7.11",
    "slate-react": "0.22.10",
    "slate-soft-break": "0.9.0",
    "styled-components": "5.2.1",
    "typeface-roboto": "1.1.13"
  },
  "devDependencies": {
    "@bundle-analyzer/webpack-plugin": "0.5.1",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-graphql-request": "2.0.3",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@next/bundle-analyzer": "10.0.3",
    "@sentry/webpack-plugin": "1.14.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/typography": "0.3.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/node-fetch": "2.5.7",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.0",
    "@types/react-color": "3.0.4",
    "@types/react-div-100vh": "0.3.0",
    "@types/react-dom": "17.0.0",
    "@types/slate": "0.47.4",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.7.0",
    "@types/slate-react": "0.22.9",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "autoprefixer": "10.1.0",
    "babel-plugin-macros": "3.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "dotenv": "8.2.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "fast-xml-parser": "3.17.5",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "next-compose-plugins": "2.2.1",
    "postcss": "8.2.1",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.2",
    "twin.macro": "2.0.7",
    "typescript": "4.1.3"
  }
