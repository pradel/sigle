{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "test": "yarn test:lint && yarn test:only",
    "analyze": "ANALYZE=true next build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,css}'",
    "test:only": "jest",
    "test:lint": "eslint 'src/**/*.{js,ts,tsx}'"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@types/slate": "0.47.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ]
  },
  "dependencies": {
    "@blockstack/connect": "2.12.4",
    "@blockstack/stacks-transactions": "0.7.0",
    "@reach/dialog": "0.11.2",
    "@reach/menu-button": "0.11.2",
    "@reach/tooltip": "0.11.2",
    "@sentry/browser": "5.24.2",
    "@sentry/integrations": "5.24.2",
    "@sentry/node": "5.24.2",
    "@tippy.js/react": "3.1.1",
    "blockstack": "21.1.1",
    "date-fns": "2.16.1",
    "fathom-client": "3.0.0",
    "feed": "4.2.1",
    "formik": "2.1.5",
    "immutable": "4.0.0-rc.12",
    "nanoid": "3.1.12",
    "next": "9.5.4",
    "next-seo": "4.10.0",
    "nprogress": "0.2.0",
    "react": "16.13.1",
    "react-color": "2.18.1",
    "react-div-100vh": "0.5.5",
    "react-dom": "16.13.1",
    "react-dropzone": "11.2.0",
    "react-icons": "3.11.0",
    "react-spring": "8.0.27",
    "react-toastify": "6.0.8",
    "slate": "0.47.9",
    "slate-html-serializer": "0.8.11",
    "slate-plain-serializer": "0.7.11",
    "slate-react": "0.22.10",
    "slate-soft-break": "0.9.0",
    "styled-components": "5.2.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@bundle-analyzer/webpack-plugin": "0.5.1",
    "@next/bundle-analyzer": "9.5.3",
    "@sentry/webpack-plugin": "1.13.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@types/nprogress": "0.2.0",
    "@types/react": "16.9.49",
    "@types/react-color": "3.0.4",
    "@types/react-div-100vh": "0.3.0",
    "@types/react-dom": "16.9.8",
    "@types/slate": "0.47.4",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.7.0",
    "@types/slate-react": "0.22.9",
    "@types/styled-components": "5.1.3",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-styled-components": "1.11.1",
    "dotenv": "8.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.2",
    "fast-xml-parser": "3.17.4",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.4.0",
    "next-compose-plugins": "2.2.0",
    "prettier": "2.1.2",
    "tailwindcss": "1.8.10",
    "twin.macro": "1.10.0",
    "typescript": "4.0.3"
  }
