{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "test": "yarn test:lint && yarn test:only",
    "analyze": "ANALYZE=true next build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,css}'",
    "test:only": "jest",
    "test:lint": "eslint 'src/**/*.{js,ts,tsx}'"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ]
  },
  "dependencies": {
