    "@sentry/browser": "5.27.2",
    "@sentry/integrations": "5.27.2",
    "@sentry/node": "5.27.2",
    "@tippy.js/react": "3.1.1",
    "blockstack": "21.1.1",
    "date-fns": "2.16.1",
    "fathom-client": "3.0.0",
    "feed": "4.2.1",
    "formik": "2.2.1",
    "immutable": "4.0.0-rc.12",
    "is-hotkey": "0.1.6",
    "lodash.isempty": "4.4.0",
    "nanoid": "3.1.16",
    "next": "10.0.0",
    "next-seo": "4.14.0",
    "nprogress": "0.2.0",
    "react": "17.0.1",
    "react-color": "2.19.3",
    "react-div-100vh": "0.5.6",
    "react-dom": "17.0.1",
    "react-dropzone": "11.2.2",
    "react-icons": "3.11.0",
    "react-spring": "8.0.27",
    "react-toastify": "6.0.9",
    "slate": "0.59.0",
    "slate-history": "0.59.0",
    "slate-react": "0.59.0",
    "styled-components": "5.2.0",
    "typeface-roboto": "1.1.13"
