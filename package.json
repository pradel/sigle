{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "analyze": "ANALYZE=true yarn build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md}'",
    "test:prettier": "prettier --list-different '**/*.{js,ts,tsx,md}'"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "blockstack": "18.3.0",
    "date-fns": "1.30.1",
    "dompurify": "1.0.9",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.12",
    "nanoid": "2.0.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-helmet": "5.2.0",
    "react-icons": "3.4.0",
    "react-router-dom": "4.3.1",
    "react-toastify": "4.5.2",
    "slate": "0.44.10",
    "slate-html-serializer": "0.7.34",
    "slate-plain-serializer": "0.6.34",
    "slate-react": "0.21.16",
    "slate-soft-break": "0.9.0",
    "styled-components": "4.1.3",
    "the-platform": "0.9.3",
    "typeface-libre-baskerville": "0.0.54",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@craco/craco": "3.4.1",
    "@types/dompurify": "0.0.32",
    "@types/jest": "24.0.5",
    "@types/nanoid": "1.2.0",
    "@types/node": "10.12.21",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "@types/react-helmet": "5.0.8",
    "@types/react-router-dom": "4.3.1",
    "@types/slate": "0.43.8",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.6.1",
    "@types/slate-react": "0.20.3",
    "@types/styled-components": "4.1.10",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "react-scripts": "2.1.5",
    "tailwind.macro": "0.5.10",
    "tailwindcss": "0.7.4",
    "typescript": "3.3.3",
    "webpack-bundle-analyzer": "3.0.4"
  }
}
